<%= render 'game_board', :game => @game %>

<script type="text/javascript">
  game.init({
    'player_id': '<%= @user.id %>'
    'player_token': '<%= @user.token(@game.id) %>',
    'move_url': '<%= j game_move_url(@game) %>',
    'winning_url': '<%= j game_winning_url(@game) %>'
    'current_board': '<%= j @game.board %>'
  });
</script>
